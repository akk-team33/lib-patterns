<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/POM/4.0.0"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>de.team33.patterns</groupId>
    <artifactId>patterns-bom</artifactId>
    <version>1.9.0</version>
    <packaging>pom</packaging>

    <name>Team33 Patterns Library BOM</name>
    <description>Team33 patterns library collection - bill of materials</description>
    <url>http://team33.de/dev/patterns/patterns-bom/</url>

    <licenses>
        <license>
            <name>The Apache License, Version 2.0</name>
            <url>http://www.apache.org/licenses/LICENSE-2.0.txt</url>
        </license>
    </licenses>
    <developers>
        <developer>
            <name>Andreas Kluge-Kaindl</name>
            <email>akk-pg@team33.de</email>
            <organization>team33.de</organization>
            <organizationUrl>https://github.com/akk-team33/</organizationUrl>
        </developer>
    </developers>
    <scm>
        <connection>scm:git:git://github.com/akk-team33/lib-patterns.git</connection>
        <developerConnection>scm:git:ssh://github.com:akk-team33/lib-patterns.git</developerConnection>
        <url>https://github.com/akk-team33/lib-patterns</url>
    </scm>

    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>de.team33.patterns</groupId>
                <artifactId>production-01</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>de.team33.patterns</groupId>
                <artifactId>random-01</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>de.team33.patterns</groupId>
                <artifactId>random-mimas</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>de.team33.patterns</groupId>
                <artifactId>random-tarvos</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>de.team33.patterns</groupId>
                <artifactId>exceptional-01</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>de.team33.patterns</groupId>
                <artifactId>lazy-01</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>de.team33.patterns</groupId>
                <artifactId>lazy-narvi</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>de.team33.patterns</groupId>
                <artifactId>pooling-01</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>de.team33.patterns</groupId>
                <artifactId>tuple-janus</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>de.team33.patterns</groupId>
                <artifactId>reflect-luna</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>de.team33.patterns</groupId>
                <artifactId>expiry-tethys</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>de.team33.patterns</groupId>
                <artifactId>properties-01</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>de.team33.patterns</groupId>
                <artifactId>testing-01</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>de.team33.patterns</groupId>
                <artifactId>testing-titan</artifactId>
                <version>${project.version}</version>
            </dependency>
        </dependencies>
    </dependencyManagement>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-gpg-plugin</artifactId>
                <version>3.0.1</version>
                <executions>
                    <execution>
                        <id>sign-artifacts</id>
                        <phase>verify</phase>
                        <goals>
                            <goal>sign</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>

    <distributionManagement>
        <snapshotRepository>
            <id>ossrh</id>
            <url>https://oss.sonatype.org/content/repositories/snapshots</url>
        </snapshotRepository>
        <repository>
            <id>ossrh</id>
            <url>https://oss.sonatype.org/service/local/staging/deploy/maven2/</url>
        </repository>
    </distributionManagement>
</project>
